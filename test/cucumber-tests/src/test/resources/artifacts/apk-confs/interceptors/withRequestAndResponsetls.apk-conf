name: "InterceptorAPI"
basePath: "/interceptor"
version: "1.0.0"
type: "REST"
defaultVersion: true
endpointConfigurations:
  production:
    endpoint: "http://backend.apk-integration-test.svc.cluster.local"
operations:
  - target: "/get"
    verb: "GET"
    authTypeEnabled: true
    scopes: []
apiPolicies:
  request:
    - policyName: "Interceptor"
      policyVersion: v1
      parameters:
        backendUrl: "https://interceptor-service.apk-integration-test.svc.cluster.local:8444"
        contextEnabled: true
        headersEnabled: true
        bodyEnabled: true
        tlsSecretName: "interceptor-service-secret"
        tlsSecretKey: "tls.pem"
  response:
    - policyName: "Interceptor"
      policyVersion: v1
      parameters:
        backendUrl: "https://interceptor-service.apk-integration-test.svc.cluster.local:8444"
        contextEnabled: true
        headersEnabled: true
        bodyEnabled: true
        tlsSecretName: "interceptor-service-secret"
        tlsSecretKey: "tls.pem"
