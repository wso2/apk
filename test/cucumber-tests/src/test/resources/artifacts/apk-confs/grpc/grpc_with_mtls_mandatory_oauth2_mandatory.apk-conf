name: "6a254687f3229c35dd0189aac7f7fc4b6228e97a"
basePath: "/org.apk"
version: "v1"
type: "GRPC"
id: "grpc-mtls-mandatory-oauth2-mandatory"
endpointConfigurations:
  production:
    endpoint: "http://grpc-backend:6565"
defaultVersion: false
subscriptionValidation: false
operations:
  - target: "student_service.StudentService"
    verb: "GetStudent"
    secured: true
    scopes: []
  - target: "student_service.StudentService"
    verb: "GetStudentStream"
    secured: true
    scopes: []
  - target: "student_service.StudentService"
    verb: "SendStudentStream"
    secured: true
    scopes: []
  - target: "student_service.StudentService"
    verb: "SendAndGetStudentStream"
    secured: true
    scopes: []
authentication:
  - authType: OAuth2
    required: mandatory
  - authType: mTLS
    required: mandatory
    certificates:
      - name: mtls-test-configmap
        key: tls.crt
