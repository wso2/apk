---
organization: "wso2"
name: "test-cors"
context: "/test_cors"
version: "2.0.0"
type: "REST"
endpointConfigurations:
    production:
        endpoint: "https://httpbin.org"
operations:
    - target: "/anything"
      verb: "GET"
      authTypeEnabled: true
      scopes: []
vhosts:
  production: ["gw.am.wso2.com"]
  sandbox: []
corsConfiguration:
  enable: true
  corsConfigurationEnabled: "asd"
  accessControlAllowOrigins:
    - "wso2.com"
  accessControlAllowCredentials: true
  accessControlAllowHeaders:
    - "Content-Type"
    - "Authorization"
  accessControlAllowMethods:
    - "GET"
  accessControlAllowMaxAge: 3600
apiPolicies:
  request:
    - policyName: "BackendJwt"
      parameters:
        enabled: true
        encoding: base64
        signingAlgorithm: SHA256withRSA
        header: X-JWT-Assertion
        tokenTTL: 3600
        customClaims:
        - claim: claim1
          value: value1
        - claim: claim2
          value: value2
