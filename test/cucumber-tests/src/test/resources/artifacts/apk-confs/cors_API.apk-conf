name: "test-cors"
id: "cors-api-adff3dbc-2787-11ee-be56-0242ac120002"
basePath: "/test_cors"
version: "2.0.0"
type: "REST"
endpointConfigurations:
    production:
      - endpoint:
          name: "backend"
          namespace: "apk-integration-test"
          port: 80
          protocol: "http"
operations:
    - target: "/anything"
      verb: "GET"
      secured: true
      scopes: []
corsConfiguration:
  corsConfigurationEnabled: true
  accessControlAllowOrigins:
    - "https://abc.com"
  accessControlAllowCredentials: true
  accessControlAllowHeaders:
    - "Content-Type"
    - "Authorization"
  accessControlAllowMethods:
    - "GET"
    - "POST"
    - "PUT"
    - "DELETE"
  accessControlAllowMaxAge: 7200
