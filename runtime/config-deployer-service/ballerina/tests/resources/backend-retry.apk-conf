---
name: "test-retry"
basePath: "/test_retry"
version: "2.0.0"
type: "REST"
endpointConfigurations:
    production:
        endpoint: "https://httpbin.org"
        resiliency:
          circuitBreaker:
            maxConnectionPools: 200
            maxConnections: 100
            maxPendingRequests: 100
            maxRequests: 100
            maxRetries: 5
          timeout:
            downstreamRequestIdleTimeout: 400
            upstreamResponseTimeout: 40
          retryPolicy:
            count: 3
            baseIntervalMillis: 1000
            statusCodes:
              - 504
operations:
    - target: "/anything"
      verb: "GET"
      authTypeEnabled: true
      scopes: []
