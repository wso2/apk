name: api1
version: 1.0.0
basePath: /api1/1.0.0
type: REST
defaultVersion: true
endpointConfigurations:
  production:
  - endpoint: http://backend.test-apk.svc.cluster.local:80
  sandbox:
  - endpoint: http://backend.test-apk.svc.cluster.local:80
operations:
- target: /get
  verb: GET
  secured: false
- target: /get
  verb: POST
authentication: 
  - authType: "APIKey"
    enabled: true
    queryParamEnable: true
corsConfiguration:
  corsConfigurationEnabled: false
  accessControlAllowOrigins:
  - '*'
  accessControlAllowCredentials: false
  accessControlAllowHeaders:
  - authorization
  - Access-Control-Allow-Origin
  - Content-Type
  - SOAPAction
  - apikey
  - testKey
  accessControlAllowMethods:
  - GET
  - PUT
  - POST
  - DELETE
  - PATCH
  - OPTIONS
  accessControlAllowMaxAge: 0
rateLimit:
  requestsPerUnit: 10
  unit: Minute