---
name: "test-cors"
basePath: "/test_cors"
version: "2.0.0"
type: "REST"
endpointConfigurations:
    production:
      endpoint: "https://backend-prod-test/v1/"
    sandbox:
      endpoint:
        name: http-bin-backend
        namespace: apk-test
        port: 8080
        protocol: http
operations:
    - target: "/anything"
      verb: "GET"
      authTypeEnabled: true
      scopes: []
apiPolicies:
  request:
    - policyName: "Interceptor"
      parameters:
        backendUrl: "http://interceptor-req-service.ns.svc.cluster.local:8443"
        bodyEnabled: true
        contextEnabled: true
        headersEnabled: true
        trailersEnabled: true
  response:
    - policyName: "Interceptor"
      policyVersion: v2
      parameters:
        backendUrl: "http://interceptor-res-service.ns.svc.cluster.local:8443"
        bodyEnabled: true
        contextEnabled: false
        headersEnabled: false
        trailersEnabled: true
